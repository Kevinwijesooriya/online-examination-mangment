<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.exapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ExApp">
        <activity
            android:name=".LectureRegistration"
            android:exported="true" />
        <activity
            android:name=".LectureLogin"
            android:exported="true" />
        <activity android:name=".QuizActivity" />
        <activity android:name=".qMainActivity" />
        <activity android:name=".StudentLanding" />
        <activity android:name=".StudentLogin" />
        <activity android:name=".StudentRegistration" />
        <activity
            android:name=".UpdateEnrollment"
            android:parentActivityName=".viewEnrollment" />
        <activity
            android:name=".viewEnrollment"
            android:label="My Enrollments" />
        <activity
            android:name=".AddEnrollment"
            android:label="New Enrollment"
            android:parentActivityName=".viewEnrollment" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>